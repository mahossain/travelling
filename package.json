{
  "name": "travelling",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "debug": "nodemon --inspect --watch src src/index.js",
    "ci": "npm run test",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahossain/travelling.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jest": "^26.0.1",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "jest": {
    "testRegex": "./src/__tests__/.*\\.(test|spec)\\.js$",
    "testEnvironment": "node",
    "setupFiles": [
      "./src/test-setup/index.js"
    ],
    "testPathIgnorePatterns": [
      "./src/__tests__/_test_setup"
    ],
    "setupTestFrameworkScriptFile": "./src/test-setup/pre-test.js",
    "globals": {
      "request": {}
    },
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "lib"
    ]
  },
  "bugs": {
    "url": "https://github.com/mahossain/travelling/issues"
  },
  "homepage": "https://github.com/mahossain/travelling#readme"
}
